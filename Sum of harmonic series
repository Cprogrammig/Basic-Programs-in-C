/*Sum of harmonic series upto the required range
   1+ 1/2 + 1/3 + 1/4 + 1/5 + .. 1/N  */
#include<stdio.h>
int main()
{
	int n,i;
	float sum=0;
	printf("Enter the range required:");
	scanf("%d",&n);
    for(i=1;i<=n;i++)
	{
		sum=sum+((float)1/(float)i);
		
		}	
		printf("Sum:%.2f",sum);
		return 0;
}
