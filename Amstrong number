/*Amstrong number or not*/
#include<stdio.h>
#include<math.h>
int main()
{
	int num,a,c,r,b;
	printf("Enter the number:\n");
	scanf("%d",&num);
	a=num;
	c=floor(log10(num))+1;
	while(num!=0)
	{
		r=num%10;
		num=num/10;
		b=b+pow(r,c);
		
	}
	if(b==a)
	printf("%d is a amstrong number",a);
	else
	printf("%d is not a amstrong number",a);
	return 0;
}
